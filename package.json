  "bugs": {
    "url": "https://github.com/rickycodes/random-404/issues"
  },
  "homepage": "https://github.com/rickycodes/random-404",
  "dependencies": {
    "express": "^4.13.4"
  },
  "devDependencies": {
    "standard": "^11.0.1",
    "typescript": "^2.8.3",
    "uglify-js": "^3.3.22"
  }
}
